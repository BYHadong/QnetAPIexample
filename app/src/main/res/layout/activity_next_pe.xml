<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="adong.org.qnetapiexample.NextPEActivity">

    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="회차 -> "
            android:textSize="16.5dp"/>

        <TextView
            android:id="@+id/description1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="회차"
            android:textSize="16.5dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="필기 원서 접수 시작 -> "
            android:textSize="16.5dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/docregstartdt1"
            android:text="필기 원서 접수 시작"
            android:textSize="16.5dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="필기 원서 접수 종료 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/docregenddt1"
            android:text="필기 원서 접수 종료"
            android:textSize="16.5dp"/>

    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="필기 시험 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/docexamdt1"
            android:text="필기 시험 일자"
            android:textSize="16.5dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="응시자격 서류제출 및 필기시험 합격자 결정시작일자 -> "
            android:textSize="16.5dp"
            android:layout_weight="0.5"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/docsubmitstartdt1"
            android:text="응시자격 서류제출 및 필기시험 합격자 결정시작일자"
            android:textSize="16.5dp"
            android:layout_weight="0.5"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="응시자격 서류제출 및 필기시험 합격자 결정종료일자 -> "
            android:textSize="16.5dp" />

        <TextView
            android:id="@+id/docsubmitendt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="응시자격 서류제출 및 필기시험 합격자 결정종료일자 "
            android:textSize="16.5dp"
            android:layout_weight="0.5"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="필기 합격자 발표 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/docpassdt1"
            android:text="필기 합격자 발표 일자"
            android:textSize="16.5dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="면접시험 원서접수 시작 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/pracregstartdt1"
            android:text="면접시험 원서접수 시작 일자 "
            android:textSize="16.5dp"/>

    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="면접시험 원서접수 종료 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/pracregenddt1"
            android:text="면접시험 원서접수 종료 일자"
            android:textSize="16.5dp"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="면접시험 시작 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/pracexamstartdt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="면접시험 시작 일자"
            android:textSize="16.5dp" />

    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="면접시험 종료 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/pracexamenddt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="면접시험 종료 일자"
            android:textSize="16.5dp" />

    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="면접시험 합격자 발표 일자 -> "
            android:textSize="16.5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/pracpassdt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="면접시험 합격자 발표 일자"
            android:textSize="16.5dp" />

    </LinearLayout>

</LinearLayout>
